{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "declaration": true,
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "allowJs": true,
    "strict": true,
    "preserveConstEnums": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "**/*.spec.ts"
  ]
}
